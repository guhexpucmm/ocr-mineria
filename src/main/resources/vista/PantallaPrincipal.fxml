<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.control.textfield.CustomTextField?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="stackPane" maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
           prefWidth="900.0" styleClass="stack-pane-principal" stylesheets="/css/main.css"
           xmlns="http://javafx.com/javafx/8.0.112"
           fx:controller="edu.pucmm.mineriadedatos2017.controladora.PantallaPrincipalControladora">
   <children>
      <VBox fx:id="vBox" alignment="CENTER" onDragDetected="#vBoxDragDetected" onDragDone="#vBoxDragDone" onDragDropped="#vBoxDragDropped" onDragEntered="#vBoxDragEntered" prefHeight="200.0" prefWidth="100.0" StackPane.alignment="CENTER">
         <children>
            <HBox fx:id="hBox" alignment="CENTER" prefHeight="0.0" prefWidth="600.0">
               <children>
                  <Button fx:id="btnEntrenarModelo" minWidth="150.0" mnemonicParsing="false" onAction="#btnEjecutarModeloClick" text="Entrenar modelo">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnReconocerLetra" minWidth="150.0" mnemonicParsing="false" onAction="#btnReconocerLetraClick" text="Reconocer letra">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox fx:id="hBox2" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <CustomTextField fx:id="txtField" minWidth="700.0" promptText="URL o Path del archivo">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" top="5.0" />
                     </HBox.margin>
                  </CustomTextField>
                  <Button fx:id="btnAnalizar" minWidth="150.0" mnemonicParsing="false" onAction="#btnAnalizarClick" text="Analizar">
                     <HBox.margin>
                        <Insets bottom="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="20.0"/>
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
